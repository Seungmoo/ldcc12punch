<script type="text/javascript">
	function chk_idform(){
		var name = document.getElementById("id_name");
		var pn_1 = document.getElementById("pn_1");
		var pn_2 = document.getElementById("pn_2");
		var pn_3 = document.getElementById("pn_3");
		var email = document.getElementById("id_email");
		if(name.value == "" || name.value == null){
			alert('이름을 입력하세요');
			name.focus();
			return false;
		}
		if(pn_1.value == "" || pn_1.value == null){
			alert('휴대번호를 입력하세요');
			pn_1.focus();
			return false;
		}
		if(pn_2.value == "" || pn_2.value == null){
			alert('휴대번호를 입력하세요');
			pn_2.focus();
			return false;
		}
		if(pn_3.value == "" || pn_3.value == null){
			alert('휴대번호를 입력하세요');
			pn_3.focus();
			return false;
		}
		if(email.value == "" || email.value == null){
			alert('이메일을 입력하세요');
			email.focus();
			return false;
		}
	}
	function chk_pwform(){
		var pid = document.getElementById("pid");
		var name = document.getElementById("name");
		var pw_email = document.getElementById("pw_email");
		if(pid.value == "" || pid.value == null){
			alert('아이디를 입력하세요');
			pid.focus();
			return false;
		}
		if(name.value == "" || name.value == null){
			alert('이름을 입력하세요');
			name.focus();
			return false;
		}
		if(pw_email.value == "" || pw_email.value == null){
			alert('이메일을 입력하세요');
			pw_email.focus();
			return false;
		}
	}
